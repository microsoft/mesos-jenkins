{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "orchestratorType": "DCOS",
      "orchestratorRelease": "",
      "dcosConfig": {
        "dcosWindowsBootstrapURL": "http://dcos-win.westus.cloudapp.azure.com/dcos-windows/testing"
      }
    },
    "masterProfile": {
      "count": 3,
      "dnsPrefix": "",
      "vmSize": "Standard_D2s_v3",
      "preProvisionExtension": {
        "name": "preprovision-master-linux",
        "singleOrAll": "All"
      }
    },
    "agentPoolProfiles": [
      {
        "name": "lnxpri",
        "count": 1,
        "vmSize": "Standard_D2s_v3",
        "preProvisionExtension": {
          "name": "preprovision-agent-linux-private",
          "singleOrAll": "All"
        },
        "customNodeLabels": {
          "infrastructure": "ci"
        }
      },
      {
        "name": "lnxpub",
        "count": 2,
        "vmSize": "Standard_D2s_v3",
        "dnsPrefix": "",
        "preProvisionExtension": {
          "name": "preprovision-agent-linux-public",
          "singleOrAll": "All"
        },
        "customNodeLabels": {
          "infrastructure": "ci"
        },
        "ports": [
          80,
          443,
          8080
        ]
      },
      {
        "name": "winpri",
        "count": 1,
        "vmSize": "Standard_D2s_v3",
        "osType": "Windows",
        "preProvisionExtension": {
            "name": "preprovision-agent-windows",
            "singleOrAll": "All"
        },
        "customNodeLabels": {
          "infrastructure": "ci"
        }
      },
      {
        "name": "winpub",
        "count": 2,
        "vmSize": "Standard_D2s_v3",
        "osType": "Windows",
        "dnsPrefix": "",
        "preProvisionExtension": {
          "name": "preprovision-agent-windows",
          "singleOrAll": "All"
        },
        "customNodeLabels": {
          "infrastructure": "ci"
        },
        "ports": [
          80,
          443,
          8080,
          3389
        ]
      }
    ],
    "windowsProfile": {
      "adminUsername": "azureuser",
      "adminPassword": ""
    },
    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": ""
          }
        ]
      }
    },
    "extensionProfiles": [
      {
        "name": "preprovision-agent-linux-public",
        "version": "v1",
        "extensionParameters": "parameters",
        "rootURL": "http://dcos-win.westus.cloudapp.azure.com/dcos-windows/testing/preprovision/",
        "script": "preprovision-agent-linux-public.sh"
      },
      {
        "name": "preprovision-agent-linux-private",
        "version": "v1",
        "extensionParameters": "parameters",
        "rootURL": "http://dcos-win.westus.cloudapp.azure.com/dcos-windows/testing/preprovision/",
        "script": "preprovision-agent-linux-private.sh"
      },
      {
        "name": "preprovision-master-linux",
        "version": "v1",
        "extensionParameters": "parameters",
        "rootURL": "http://dcos-win.westus.cloudapp.azure.com/dcos-windows/testing/preprovision/",
        "script": "preprovision-master-linux.sh"
      },
      {
        "name": "preprovision-agent-windows",
        "version": "v1",
        "extensionParameters": "parameters",
        "rootURL": "http://dcos-win.westus.cloudapp.azure.com/dcos-windows/testing/preprovision/",
        "script": "preprovision-agent-windows.ps1"
      }
    ]
  }
}
